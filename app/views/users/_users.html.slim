- if users.any?
  - users.each do |user|
    hr
    = user.name
    - if user.user_type == 1
      p 生徒
    - elsif user.user_type == 2
      p 先生
    = user.title
      br
    = user.content
      br
    = link_to "興味ある", new_interest_path(user)
  - if current_user
    = link_to "編集", edit_user_path(current_user)
    = link_to "削除", user_path(current_user), :method => :delete, data: {confirm: "削除しますか？"}
